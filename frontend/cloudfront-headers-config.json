{
  "Comment": "COEP Headers para Seeso.io - Frontend EmotioXV2",
  "ResponseHeadersPolicyConfig": {
    "Name": "emotioxv2-frontend-security-headers",
    "Comment": "Security headers including COEP for Seeso.io eye tracking",
    "CustomHeadersConfig": {
      "Quantity": 2,
      "Items": [
        {
          "Header": "Cross-Origin-Opener-Policy",
          "Value": "same-origin",
          "Override": true
        },
        {
          "Header": "Cross-Origin-Embedder-Policy",
          "Value": "credentialless",
          "Override": true
        }
      ]
    },
    "SecurityHeadersConfig": {
      "StrictTransportSecurity": {
        "AccessControlMaxAgeSec": 63072000,
        "IncludeSubdomains": true,
        "Override": true
      },
      "ContentTypeOptions": {
        "Override": true
      },
      "FrameOptions": {
        "FrameOption": "DENY",
        "Override": true
      },
      "XSSProtection": {
        "ModeBlock": true,
        "Protection": true,
        "Override": true
      },
      "ReferrerPolicy": {
        "ReferrerPolicy": "strict-origin-when-cross-origin",
        "Override": true
      }
    },
    "CorsConfig": {
      "AccessControlAllowOrigins": {
        "Quantity": 1,
        "Items": ["*"]
      },
      "AccessControlAllowHeaders": {
        "Quantity": 1,
        "Items": ["*"]
      },
      "AccessControlAllowMethods": {
        "Quantity": 4,
        "Items": ["GET", "HEAD", "OPTIONS", "POST"]
      },
      "AccessControlAllowCredentials": false,
      "OriginOverride": true
    }
  }
}
