{
  "name": "emotioxv2",
  "version": "2.0.0",
  "description": "Sistema completo de investigación y análisis de emociones con eye-tracking - Monorepo",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "serverless offline start",
    "build": "npm run build-all",
    "deploy": "serverless deploy",
    "deploy:prod": "serverless deploy --stage prod",
    "lint": "eslint . --ext .ts",
    "test": "jest --passWithNoTests",
    "prepare": "husky || true",
    "quality-check": "./scripts/check-quality.sh",
    "pre-commit-test": "npm run quality-check",
    "type-check": "npx tsc --noEmit",
    "check-types-all": "npm-run-all --parallel check-types:backendV2 check-types:shared check-types:public-tests --sequential check-types:frontend",
    "check-types:backendV2": "cd backendV2 && npx tsc --noEmit",
    "check-types:frontend": "cd frontend && npm run build > /dev/null 2>&1",
    "check-types:shared": "cd shared && npx tsc --noEmit",
    "check-types:public-tests": "cd public-tests && npx tsc --noEmit",
    "lint-all": "npm-run-all --parallel lint:backendV2 lint:frontend lint:shared lint:public-tests",
    "lint:backendV2": "cd backendV2 && npm run lint",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:shared": "cd shared && npm run lint",
    "lint:public-tests": "cd public-tests && npm run lint",
    "build-all": "npm-run-all --sequential build:shared build:backendV2 --parallel build:frontend build:public-tests",
    "build:backendV2": "cd backendV2 && npx tsc",
    "build:frontend": "cd frontend && npm run build",
    "build:shared": "cd shared && npx tsc",
    "build:public-tests": "cd public-tests && npm run build",
    "check-no-any": "grep -rE ':\\s*any(\\[\\]|>|\\s|$)' --include='*.ts' --include='*.tsx' --exclude-dir=node_modules --exclude-dir=.next --exclude-dir=dist --exclude-dir=build . 2>/dev/null | grep -vE '(//.*any|\\.d\\.ts:)' || echo '✅ No any types found'",
    "check-no-unknown": "grep -rE ':\\s*unknown(\\[\\]|>|\\s|$)' --include='*.ts' --include='*.tsx' --exclude-dir=node_modules --exclude-dir=.next --exclude-dir=dist --exclude-dir=build . 2>/dev/null | grep -vE '(isValidUser|parseJwt|//.*unknown|\\.d\\.ts:)' || echo '✅ No unknown types found'"
  },
  "keywords": [
    "emotion-research",
    "eye-tracking",
    "research-platform",
    "monorepo",
    "typescript",
    "nextjs",
    "serverless",
    "aws-lambda"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "."
  },
  "type": "commonjs",
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "^3.777.0",
    "@types/aws-lambda": "^8.10.147",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.11",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "eslint": "^9.23.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "serverless": "^3.40.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-export-outputs": "^1.0.2",
    "serverless-offline": "^13.9.0",
    "serverless-plugin-export-endpoints": "^0.1.3",
    "ts-jest": "^29.2.6",
    "typescript": "^5.8.2",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.190.0",
    "@aws-sdk/client-dynamodb": "^3.190.0",
    "@aws-sdk/client-s3": "^3.190.0",
    "@aws-sdk/client-ses": "^3.190.0",
    "@aws-sdk/lib-dynamodb": "^3.190.0",
    "@aws-sdk/s3-request-presigner": "^3.190.0",
    "axios": "^1.8.4",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.0"
  },
  "private": true,
  "workspaces": [
    "shared",
    "frontend",
    "public-tests"
  ]
}
